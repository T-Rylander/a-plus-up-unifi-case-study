# Environment Variables Template
# Copy this file to .env and fill in your credentials
# NEVER commit .env to git (it's in .gitignore)

# UDM Pro Max Credentials
UDM_HOST=192.168.1.1
UDM_USER=admin
UDM_PASS=your-secure-password-here

# Backup Server (Optional)
BACKUP_HOST=backup.example.com
BACKUP_USER=backup
BACKUP_PASS=your-backup-password

# Verkada API (Optional)
VERKADA_API_KEY=your-verkada-api-key

# Spectrum SIP (Optional)
SIP_TRUNK=sip.spectrum.net
SIP_PORT=5060
SIP_USER=your-sip-username
SIP_PASS=your-sip-password

# Notification (Optional)
SLACK_WEBHOOK=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
EMAIL_ALERT=admin@aplusup.edu
